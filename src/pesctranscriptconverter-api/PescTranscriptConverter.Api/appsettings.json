{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GotenbergSharpClient": {
    "RetryPolicy": {
      "Enabled": true,
      "RetryCount": 4,
      "BackoffPower": 1.5,
      "LoggingEnabled": true
    }
  },
  "CdlAssets": {
    "RootDirectory": "./cdl"
  },
  "PdfAssets": {
    "RootDirectory": "./pdf",
    "PdfHeader": "header.html",
    "PdfFooter": "footer.html"
  }
}
